    "animate.css": "^4.1.1",
    "astro": "^4.14.2",
    "astro-github-stats": "^0.7.0",
    "marked": "^14.1.2",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "eslint": "^9.9.1",
    "husky": "7.0.4",
    "lint-staged": "15.2.9",
    "prettier": "3.1.1",
    "sass-embedded": "^1.77.8"
