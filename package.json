{
  "name": "negocio",
  "type": "module",
  "version": "0.0.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "scripts": {
    "lint:staged": "lint-staged",
